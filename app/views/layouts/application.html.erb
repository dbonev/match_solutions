    <!-- ******FOOTER****** --> 
    <footer class="footer">
        <div class="container text-center">
            <small class="copyright">Designed with <i class="fa fa-heart"></i> by <a href="http://themes.3rdwavemedia.com" target="_blank">Xiaoying Riley</a> for developers</small>
        </div><!--//container-->
    </footer><!--//footer-->


	
<h1>Outsourcing Heaven</h1>
	<div class="container">
		<div id="navbar" class="navbar-header">
		<div class="navbar-collapse">
			<%= link_to "home", pages_path %>
			<%= link_to "faq", faq_path %>
			<% if @current_user %>
				<% user_email = @current_user.email %>
				Logged in as <%= link_to user_email, user_path %> <%= link_to "logout", :controller => :sessions, :action => :destroy %>
			<% else %>
				<%= link_to "login", login_path(:rurl => request.original_url) %>
			<% end %>		
		</div>
		</div>
	</div>
	<div class="container">
		<%= yield %>
	</div>
</body>
</html>
